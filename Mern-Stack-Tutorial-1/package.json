{
  "name": "mern_shopping_list",
  "version": "1.0.0",
  "description": "Shopping list built with MERN stack by following youtube tutorial Learn the MERN Stack by Traversary Media",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uk96/Tutorial-Project.git"
  },
  "author": "Uddesh Kadu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uk96/Tutorial-Project/issues"
  },
  "homepage": "https://github.com/uk96/Tutorial-Project#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "express": "^4.17.3",
    "mongoose": "^5.7.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.2"
  }
}
